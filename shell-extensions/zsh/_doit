#compdef doit
#
# zsh completion script for doit.
#
# Dev hints:
# $CURRENT is the number of the argument being completed.
# $words is an array of words

_doit() {
	local curcontext="$curcontext"
	typeset -A opt_args

    if (( $CURRENT == 2 )); then
        _arguments "`doit-zsh dodo.py`"
    else
        _arguments "`doit-zsh dodo.py ${words[2]}`"
    fi
}

_doit "$@"
